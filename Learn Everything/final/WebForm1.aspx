            "HOW TO DO OIL PAINTING",
            "HOW TO DO CONTEMPORARY DANCING",
            "COOKING CHINEESE CUISINE",
            "PLAYING PIANO",
            "HOW TO DO FUNCTIONAL CRAFTING",
            "HOW TO PLAY TENNIS",
            "HOW TO DO CODING IN JAVA",
            "INTEGRATION IN MATHS",
            "HOW TO DO OIL PAINTING",
            "HOW TO DO CONTEMPORARY DANCING",
            "COOKING CHINEESE CUISINE",
            "PLAYING PIANO",
            "HOW TO DO FUNCTIONAL CRAFTING",
            "HOW TO PLAY TENNIS",
            "HOW TO DO CODING IN JAVA","HOW TO DO CONTEMPORARY DANCING",
            "COOKING CHINEESE CUISINE",
            "PLAYING PIANO",
            "HOW TO DO FUNCTIONAL CRAFTING",
            "HOW TO PLAY TENNIS",
            "HOW TO SPEAK ENGLISH WITH RIGHT VOCABULARY"


        ];

        printPhrases(phrases, $('#search'));
    }
        run();

